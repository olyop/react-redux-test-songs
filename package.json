{
  "private": true,
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "boilerplate",
  "scripts": {
    "build": "cls && webpack --env.mode production",
    "watch": "cls && webpack --watch --env.mode production",
    "dev-server": "cls && webpack-dev-server --env.mode development",
    "server": "cls && cd src/server && nodemon bin/www && cd ../..",
    "updates": "git checkout . && git remote update && git pull",
    "push": "git add -A && git commit -m \"Update\" && git push -u origin master",
    "dev": "concurrently \"npm run dev-server\" \"npm run server\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olyop/boilerplate.git"
  },
  "keywords": [],
  "author": "Oliver Plummer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olyop/boilerplate/issues"
  },
  "homepage": "https://github.com/olyop/boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "clean-webpack-plugin": "^2.0.2",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "helmet": "^3.18.0",
    "http-errors": "^1.7.2",
    "lodash": "^4.17.11",
    "mongoose": "^5.5.6",
